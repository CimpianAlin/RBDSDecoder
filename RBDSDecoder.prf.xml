<properties>
  <struct id="RBDS_Output" mode="readonly" name="RBDS_Output">
    <description>A structure representing the format of the output messages</description>
    <simple id="RBDS_Output::Full_Text" name="Full_Text" type="string">
      <description>The RadioText message</description>
    </simple>
    <simple id="RBDS_Output::Short_Text" name="Short_Text" type="string">
      <description>The shorter, scrolling message</description>
    </simple>
    <simple id="RBDS_Output::Station_Type" name="Station_Type" type="string">
      <description>The content type of the station (Top 40, Country, Rock, etc)</description>
    </simple>
    <simple id="RBDS_Output::Call_Sign" name="Call_Sign" type="string">
      <description>The call sign of the station</description>
    </simple>
    <simple id="RBDS_Output::PI_String" name="PI_String" type="string">
      <description>The Program Identification code of the currently playing program.  Can be used for Alternate Frequency applications</description>
    </simple>
    <simple id="RBDS_Output::Group" name="Group" type="string">
      <description>The group specifier, indicating the message content</description>
    </simple>
    <simple id="RBDS_Output::TextFlag" name="TextFlag" type="string">
      <description>The group type specifier, which can be either A or B.  This switches to wipe the previous message contents</description>
    </simple>
    <configurationkind kindtype="message"/>
  </struct>
</properties>
